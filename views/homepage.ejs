<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>REALestate Homepage</title>
    <link rel="stylesheet" href="/css/lib/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <script src="/js/lib/jQuery-3.4.1.js"></script>
    <script src="/js/lib/bootstrap.min.js"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA5AtkBvlXeI567r7_tm3JlcYivAmfEmxs&callback=initAutocomplete&libraries=places&v=weekly" defer></script>
    <script src="/js/map.js"></script>
</head>

<div id="nav-placeholder"></div>
<script>
    $(function(){
      $("#nav-placeholder").load("/nav");
    });
</script>

<body class="bodyNav">
  <div class="container-map">
    <div class="row justify-content-md-center h-100">
      <div id ="HM" class="col">
        <div id="map"></div>
      </div>
      <div id ="HM" class="col col-lg-4 h-100">
        <div id="HC" class="card ">
          <div class="card-body">
              <div class="row">
                <input id="pac-input" class="form-control" type="text-search" placeholder="Search"/>
              </div>
              <hr></hr>
              <div class="row overflow-hidden">
                <div class="col-3">
                  <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#filterModal">Filters</button>
                  <div class="modal fade" id="filterModal" tabindex="-1" role="dialog" aria-labelledby="filterModalTitle" aria-hidden="true">
                    <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title" id="filterModalTitle">Filters</h5>
                          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                          </button>
                        </div>
                        <div class="modal-body">
                          <!-- modal-body -->
                          <div class="container">
                            <div class="row" style="height: 400px;">
                              <div class="col col-lg-5">
                              <div id="filter-list" class="list-group">
                                <nav class="nav nav-pills flex-column">
                                    <a class="nav-link" href="#services" >Services</a>
                                    <a class="nav-link" href="#travel" >Travel</a>
                                    <a class="nav-link" href="#entertainment" >Entertainment</a>
                                    <a class="nav-link" href="#store" >Store (Food)</a>
                                    <a class="nav-link" href="#general" >General Shopping</a>
                                    <a class="nav-link" href="#other" >Other</a>
                                </nav>
                              </div>
                            </div>
                            <div id="filterCOL" class="col">
                              <div data-spy="scroll" data-target="#filter-list" data-offset="0">
                                <!-- Services -->
                                <h4 id="services">Services</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <% for(var i in services) {%>
                                    <li>
                                      <input type="checkbox" id="<%=services[i].value%>" name="<%=services[i].name%>" value="<%=services[i].value%>" onclick="inputType('<%=services[i].value%>')">
                                      <label for=""><%=services[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                                <!-- Travel -->
                                <h4 id="travel">Travel</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <%for(var i in travel) {%>
                                    <li>
                                      <input type="checkbox" id="<%=travel[i].value%>" name="<%=travel[i].name%>" value="<%=travel[i].value%>" onclick="inputType('<%=travel[i].value%>')">
                                      <label for=""><%=travel[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                                <!-- Entertainment -->
                                <h4 id="entertainment">Entertainment</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <%for(var i in entertainment) {%>
                                    <li>
                                      <input type="checkbox" id="<%=entertainment[i].value%>" name="<%=entertainment[i].name%>" value="<%=entertainment[i].value%>" onclick="inputType('<%=entertainment[i].value%>')">
                                      <label for=""><%=entertainment[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                                <!-- Store (Food) -->
                                <h4 id="store">Store (Food)</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <%for(var i in store) {%>
                                    <li>
                                      <input type="checkbox" id="<%=store[i].value%>" name="<%=store[i].name%>" value="<%=store[i].value%>" onclick="inputType('<%=store[i].value%>')">
                                      <label for=""><%=store[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                                <!-- General Shopping -->
                                <h4 id="general">General Shopping</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <%for(var i in general) {%>
                                    <li>
                                      <input type="checkbox" id="<%=general[i].value%>" name="<%=general[i].name%>" value="<%=general[i].value%>" onclick="inputType('<%=general[i].value%>')">
                                      <label for=""><%=general[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                                <!-- Other -->
                                <h4 id="other">Other</h4>
                                <hr>
                                <ul id="filterList" class="list-group">
                                  <%for(var i in other) {%>
                                    <li>
                                      <input type="checkbox" id="<%=other[i].value%>" name="<%=other[i].name%>" value="<%=other[i].value%>" onclick="inputType('<%=other[i].value%>')">
                                      <label for=""><%=other[i].name%></label>
                                    </li>
                                  <% } %>
                                </ul>
                              </div>
                            </div>
                            </div>
                          </div>
                        </div>
                        <!-- modal-body-end -->
                        <div class="modal-footer">
                          <button type="button" class="btn btn-danger" onclick="clearTypes()">Clear Filters</button>
                          <button type="button" class="btn btn-success" data-dismiss="modal" onclick="getLocations()">Add Filters</button>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-9 d-flex justify-content-end">
                  <input type="button" value='Add Radius' class="btn btn-secondary btn-sm" onclick="addRadius(document.getElementById('miles').value)"></input>
                  <input type="number" id="miles" name="miles" maxlength="3" size="3" min="1" max="300" placeholder="Miles"/>
                  
                </div>
              </div>
              <hr></hr>
              <div class="row flex-grow-1 overflow-auto" id="results_div">
                <ul class="list-group" style="width: 100%;">
                  <li class="list-group-item list-group-item-dark" style="width: 100%;">Results</li>
                </ul>
              </div>
              <div class="row absolute-bottom">
                <div class="col">
                  <button type="button" class="btn btn-info btn-sm">key</button>
                </div>
                <div class="col d-flex justify-content-end">
                  <button type="button"  class="btn btn-info btn-sm">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-download" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                      <path fill-rule="evenodd" d="M.5 9.9a.5.5 0 0 1 .5.5v2.5a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2.5a.5.5 0 0 1 1 0v2.5a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2v-2.5a.5.5 0 0 1 .5-.5z"/>
                      <path fill-rule="evenodd" d="M7.646 11.854a.5.5 0 0 0 .708 0l3-3a.5.5 0 0 0-.708-.708L8.5 10.293V1.5a.5.5 0 0 0-1 0v8.793L5.354 8.146a.5.5 0 1 0-.708.708l3 3z"/>
                    </svg>

                  </button>
                </div>
              </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>