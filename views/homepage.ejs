<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>REALestate Homepage</title>
    <link rel="stylesheet" href="/css/lib/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <script src="/js/lib/jQuery-3.4.1.js"></script>
    <script src="/js/lib/bootstrap.min.js"></script>
    <script src="https://polyfill.io/v3/polyfill.min.js?features=default"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA5AtkBvlXeI567r7_tm3JlcYivAmfEmxs&callback=initAutocomplete&libraries=places&v=weekly" defer></script>
    <script src="/js/map.js"></script>


</head>

<div id="nav-placeholder"></div>
<script>
    $(function(){
      $("#nav-placeholder").load("/nav");
    });
</script>

<body class="bodyNav">

  <div class="container-map">
    <div class="row justify-content-md-center">
      
      <div id ="HM" class="col">
        <div id="map"></div>
      </div>

      <div id ="HM" class="col col-lg-4">
        <div id="HC" class="card">
          <div class="card-body">
            <div class="e-profile">

              <div class="row">
                <input id="pac-input" class="form-control" type="text-search" placeholder="Search"/>
              </div>

              <hr></hr>
              <div class="row overflow-hidden">
                <div class="col-3">
                  <button type="button" class="btn btn-primary btn-sm" data-toggle="modal" data-target="#filterModal">Filters</button>

                  <div class="modal fade" id="filterModal" tabindex="-1" role="dialog" aria-labelledby="filterModalTitle" aria-hidden="true">
                    <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
                      <div class="modal-content">
                        <div class="modal-header">
                          <h5 class="modal-title" id="filterModalTitle">Filters</h5>
                          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                          </button>
                        </div>
                        <div class="modal-body">

    <!-- modal-body -->
                          <div class="pillTabs d-flex justify-content-center d-flex align-items-center">
                            <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
                              <li class="nav-item">
                                <a class="nav-link active" id="pills-Service-tab" data-toggle="pill" href="#Service" role="tab" aria-controls="Service" aria-selected="true">Service</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Travel-tab" data-toggle="pill" href="#Travel" role="tab" aria-controls="Travel" aria-selected="false">Travel</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Entertainment-tab" data-toggle="pill" href="#Entertainment" role="tab" aria-controls="Entertainment" aria-selected="false">Entertainment</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Food-tab" data-toggle="pill" href="#Food" role="tab" aria-controls="Food" aria-selected="false">Store (food)</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-GeneralShopping-tab" data-toggle="pill" href="#GeneralShopping" role="tab" aria-controls="GeneralShopping" aria-selected="false">General Shopping</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Religious-tab" data-toggle="pill" href="#Religious" role="tab" aria-controls="Religious " aria-selected="false">Religious </a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Government-tab" data-toggle="pill" href="#Government" role="tab" aria-controls="Government" aria-selected="false">Government</a>
                              </li>
                              <li class="nav-item">
                                <a class="nav-link" id="pills-Education-tab" data-toggle="pill" href="#Education" role="tab" aria-controls="Education" aria-selected="false">Education</a>
                              </li>
                            </ul>
                          </div>

                          <hr class="FilterHR">
                            <div class="tab-content" id="pills-tabContent">
                              <div class="tab-pane fade show active" id="Service" role="tabpanel" aria-labelledby="pills-Service-tab">Service</div>
                              <div class="tab-pane fade" id="Travel" role="tabpanel" aria-labelledby="pills-Travel-tab">Travel</div>
                              <div class="tab-pane fade" id="Entertainment" role="tabpanel" aria-labelledby="pills-Entertainment-tab">Entertainment</div>
                              <div class="tab-pane fade" id="Food" role="tabpanel" aria-labelledby="pills-Food-tab">Food</div>
                              
                              <div class="tab-pane fade" id="GeneralShopping" role="tabpanel" aria-labelledby="pills-GeneralShopping-tab">GeneralShopping</div>
                              <div class="tab-pane fade" id="Religious" role="tabpanel" aria-labelledby="pills-Religious-tab">Religious</div>
                              <div class="tab-pane fade" id="Government" role="tabpanel" aria-labelledby="pills-Government-tab">Government</div>
                              <div class="tab-pane fade" id="Education" role="tabpanel" aria-labelledby="pills-Education+-tab">Education</div>
                            </div>
                        
    <!-- modal-body-end -->

                        </div>
                        <div class="modal-footer">
                          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                          <button type="button" class="btn btn-primary">Save changes</button>
                        </div>
                      </div>
                    </div>
                  </div>


                </div>
                <div class="col-9 d-flex justify-content-end">
                  <button type="button" class="btn btn-secondary btn-sm">Add Radius</button>
                  <input type="number" id="miles" name="miles" maxlength="3" size="3" min="1" max="300" placeholder="Miles"/>
                </div>
                

              </div>

              <hr></hr>


            </div>
          </div>
        </div>
      </div>


    </div>
  </div>
