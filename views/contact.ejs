<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>REALestate Contact</title>
    <link rel="stylesheet" href="/css/lib/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
    <script src="/js/lib/jQuery-3.4.1.js"></script>
    <script src="/js/lib/bootstrap.min.js"></script>
</head>

<nav class="navbar fixed-top navbar-light bg-info">
    <!-- <div class="navbar-left">
        <a href="homepage" class="btn btn-secondary" role="button">Homepage</a>
    </div> -->

    <div class="navbar-left">
        <a> Active User: Steve Rich</a>
    </div>
    
    <p class="navbar-title">REALestate</p>
    
    <div class="navbar-right ">
        <div class="btn-group ">
            <button type="button" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown">
              Dropdown
            </button>
            <div class="dropdown-menu dropdown-menu-right p-3 mb-2 bg-light text-white">
                <a class="dropdown-item" href="homepage">Homepage</a>
                <a class="dropdown-item" href="profile">Profile</a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item text-danger" href="login">Logout</a>           
             </div>
        </div>
    </div>
  </nav>

  <body class="bodyNav">
    <div class="container">
        <div class="row flex-lg-nowrap">
          <div class="col">
            <div class="row">
              <div class="col mb-3">
                <div class="card">
                  <div id="tableDiv" class="contact-body table-responsive-lg">
                    <table class="table table-striped">
                        <thead>
                          <tr class="bg-info">
                            <th scope="col">Name</th>
                            <th scope="col">Role</th>
                            <th scope="col">Phone</th>
                            <th scope="col">Email</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for( let i in user) { %>
                            <tr>
                              <th scope="row"><%= user[i].name%></th>
                              <td><%= user[i].rank%></td>
                              <td><%= user[i].phone%></td>
                              <td><%= user[i].email%></td>
                            </tr>
                          <% } %>
                        </tbody>
                      </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
    </div>
    
  </body>